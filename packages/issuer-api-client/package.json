{
    "name": "@energyweb/issuer-api-client",
    "version": "0.0.1",
    "description": "Client library interacting with the Issuer API",
    "homepage": "https://github.com/energywebfoundation/origin/tree/master/packages/issuer-api-client#readme",
    "author": "EnergyWeb DevHub GmbH; Joseph Bagaric, joseph.bagaric@energyweb.org",
    "license": "GPL-3.0-or-later",
    "main": "dist/js/src/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/energywebfoundation/origin.git"
    },
    "bugs": {
        "url": "https://github.com/energywebfoundation/origin/issues"
    },
    "scripts": {
        "build": "yarn build:client && yarn build:ts",
        "build:ts": "tsc -b tsconfig.json",
        "build:client": "yarn client:generate:schema && yarn client:generate && yarn client:clean",
        "prettier": "prettier --write --config-precedence file-override './src/**/*'",
        "lint": "eslint \"src/**/*{.ts,.tsx}\" \"test/**/*{.ts,.tsx}\"",
        "lint-fix": "eslint \"src/**/*{.ts,.tsx}\" \"test/**/*{.ts,.tsx}\" --fix",
        "clean": "shx rm -rf dist dist-shakeable",
        "client:generate": "openapi-generator generate -g typescript-axios -i src/schema.yaml -o src --api-name-suffix Client --remove-operation-id-prefix",
        "client:generate:schema": "ts-node src/generateSchema.ts",
        "client:clean": "find src/ -type f  ! -name \"*.ts\"  -delete"
    },
    "dependencies": {
        "@energyweb/issuer-api": "0.0.1",
        "@nestjs/testing": "7.4.4",
        "@nestjs/swagger": "4.6.1",
        "@nestjs/typeorm": "7.1.4",
        "@openapitools/openapi-generator-cli": "1.0.18-4.3.1",
        "axios": "0.20.0",
        "json-to-pretty-yaml": "1.2.2"
    },
    "devDependencies": {
        "prettier": "2.0.5",
        "ts-node": "8.10.2"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "files": [
        "dist"
    ],
    "resolutions": {
        "tslib": "1.13.0"
    }
}
